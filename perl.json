{
	"main" : {
		"prefix" : "main",
		"body" : [
			"#!/usr/bin/perl",
			"",
			"sub main {",
			"    $1",
			"}",
			"main();"
		]
	},
	"use_integer" : {
		"prefix" : "use_i",
		"body" : [
			"use integer;"
		]
	},
	"bigint" : {
		"prefix" : "ll",
		"body" : [
			"use bigint lib => 'GMP';"
		]
	},
	"mod_1001001001" : {
		"prefix" : "mod",
		"body" : [
			"my \\$MOD = 1001001001;"
		]
	},
	"STDIN" : {
		"prefix" : "in",
		"body" : [
			"<STDIN>;"
		]
	},
	"STDINs" : {
		"prefix" : "ins",
		"body" : [
			"split \/\\s+\/, <STDIN>;"
		]
	},
	"endl" : {
		"prefix" : "endl",
		"body" : [
			" . \"\\n\""
		]
	},
	"pfor" : {
		"prefix" : "pfor",
		"body" : [
			"for my \\$${1:i} (${2:0..\\$${3:n-1}}) {",
			"    $4",
			"}"
		]
	},
	"for" : {
		"prefix" : "for",
		"body" : [
			"for (my \\$${1:i} = ${2:0}; \\$${1:i} < \\$${3:n}; ++\\$${1:i}) {",
			"    $4",
			"}"
		]
	},
	"foreach" : {
		"prefix" : "each",
		"body" : [
			"foreach (@$1) {",
			"    $2",
			"}"
		]
	},
	"print" : {
		"prefix" : "print",
		"body" : [
			"print $1 . \"\\n\";"
		]
	},
	"sub" : {
		"prefix" : "sub",
		"body" : [
			"sub $1 {",
			"    my $2 = @_;",
			"}"
		]
	},
	"swap" : {
		"prefix" : "swap",
		"body" : [
			"sub swap {",
			"    my ($list_ref, $index1, $index2) = @_;",
			"    @$list_ref[$index1, $index2] = @$list_ref[$index2, $index1];",
			"}"
		]
	},
	"sort" : {
		"prefix" : "sort",
		"body" : [
			"$1 = sort {$a <=> $b } @$1;"
		]
	},
	"rsort" : {
		"prefix" : "rsort",
		"body" : [
			"$1 = sort {$b <=> $a } @$1;"
		]
	}
}