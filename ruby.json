{
	"int_input" : {
		"prefix" : "iget",
		"body" : [
			"gets.to_i"
		]
	},
	"string_input" : {
		"prefix" : "sget",
		"body" : [
			"gets.chomp"
		]
	},
	"split_input" : {
		"prefix" : "gets",
		"body" : [
			"gets.split"
		]
	},
	"split_map_input" : {
		"prefix" : "igets",
		"body" : [
			"gets.split.map(&:to_i)"
		]
	},
	"times_input" : {
		"prefix" : "tget",
		"body" : [
			"n.times.map{gets.split}"
		]
	},
	"int_times_input" : {
		"prefix" : "itget",
		"body" : [
			"n.times.map{gets.split.map(&:to_i)}"
		]
	},
	"for" : {
		"prefix" : "for",
		"body" : [
			"for ${1:i} in ${2:0..n}",
			"  $3",
			"end"
		]
	},
	"each" : {
		"prefix" : "each",
		"body" : [
			"$1.each do |${2:i}|",
			"  ",
			"end"
		]
	},
	"times" : {
		"prefix" : "times",
		"body" : [
			"$1.times do |${2:i}|",
			"  $3",
			"end"
		]
	},
	"Array" : {
		"prefix" : "arr",
		"body" : [
			"Array.new($1)"
		]
	},
	"2Array" : {
		"prefix" : "aaray",
		"body" : [
			"Array.new($1) { Array.new$2 }"
		]
	}
}